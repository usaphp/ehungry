  %body.bg-pizza
    // Show chrome frame if browser is lower than IE 7
    /[if lt IE 7] <p class="chromeframe">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> or <a href="http://www.google.com/chromeframe/?redirect=true">activate Google Chrome Frame</a> to improve your experience.</p>
    .navbar.navbar-inverse
      .navbar-inner
        .container
          %a.btn.btn-navbar{ "data-toggle" => "collapse", "data-target" => ".nav-collapse" }
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          %a.brand{ :href => "index.html" }
            %i.icon-cloud
            Classic Pizza
          .nav-collapse.collapse
            %ul.nav
              %li
                %a{ :href => "index.html" }
                  %i.icon-home
                  Home
              %li
                %a{ :href => "menu.html" }
                  %i.icon-th-list
                  Menu
          .top-buttons-w.pull-right
            %a.btn.btn-primary{:href => "login.html"}
              %i.icon-user
              Login
            %a.btn.btn-warning{:href => "cart.html"}
              %i.icon-ok-sign
              My Order
    .container
      .row-custom
        .size-has-right-panel
          .main-content-w
            .padded
              %h1.main-header.has-sub-header.first-header Online Menu
              %h4.sub-header Please select a menu category from the list below
              .menu-cats-w
                %ul.menu-cats
                  %li
                    %a.menu-cat{:href => "category.html"} Appetizers
                    %p.menu-cat-desc Curabitur scelerisque interdum semper. Donec sollicitudin venenatis ultrices.
                  %li
                    %a.menu-cat{:href => "category.html"} Salads
                    %p.menu-cat-desc Lorem ipsum dolor sit amet, consectetur adipiscing elit. Vivamus lobortis.
